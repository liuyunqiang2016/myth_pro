<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.viatt.zhjtpro.bo">
	<class name="TblBillsDimBo" table="tbl_bills_dim">
		<composite-id>
			<key-property name="billCode" column="bill_code" type="java.lang.String" />
			<key-property name="itemCode" column="item_code" type="java.lang.String" />
			<key-property name="roomCode" column="room_code" type="java.lang.String" />
		</composite-id>

		<property name="billAmt" column="bill_amt" type="java.lang.String" />
		<property name="jfDate" column="jf_date" type="java.lang.String" />
		<property name="jzDate" column="jz_date" type="java.lang.String" />
		<property name="isPay" column="ispay" type="java.lang.String" />
		<property name="commID" column="comm_code" type="java.lang.String" />
		<property name="areaID" column="area_code" type="java.lang.String" />
		<property name="unitID" column="unit_code" type="java.lang.String" />
		<property name="buldID" column="building_code" type="java.lang.String" />
	</class> 
</hibernate-mapping>
