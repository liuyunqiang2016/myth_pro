<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.viatt.zhjtpro.bo">
	<class name="TblInnerEquOpBo" table="tbl_inner_equ_op">
		<composite-id>
			<key-property name="innerid" column="innerid" type="java.lang.String" />
			<key-property name="outerid" column="outerid" type="java.lang.String" />
			<key-property name="optype" column="optype" type="java.lang.String" />
		</composite-id>

		<property name="remark" column="remark" type="java.lang.String" />
		<property name="bak1" column="bak1" type="java.lang.String" />
		<property name="bak2" column="bak2" type="java.lang.String" />
	</class> 
</hibernate-mapping>
