<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.viatt.zhjtpro.bo">
	<class name="TblFingDimBo" table="tbl_fing_dim">
		<id name="usrNo" column="usr_no" type="java.lang.String">
			<generator class="assigned" />
		</id>

		<property name="fingUsr" column="fing_usr" type="java.lang.String" />
