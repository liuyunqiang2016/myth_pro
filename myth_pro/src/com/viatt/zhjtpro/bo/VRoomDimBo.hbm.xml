<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.viatt.zhjtpro.bo">
	<class name="VRoomDimBo" table="v_room">
		<composite-id>
			<key-property name="roomCode" column="room_code" type="java.lang.String" />
			<key-property name="unitCode" column="unit_code" type="java.lang.String" />
			<key-property name="buildingCode" column="building_code" type="java.lang.String" />
			<key-property name="areaCode" column="area_code" type="java.lang.String" />
			<key-property name="commCode" column="comm_code" type="java.lang.String"/>
			<key-property name="htCode" column="ht_code" type="java.lang.String"/>
		</composite-id>

		<property name="roomUsr" column="room_usr" type="java.lang.String" />
		<property name="roomArea" column="room_area" type="java.lang.String" />
		<property name="ownerCode" column="owner_Code" type="java.lang.String" />
		<property name="roomState" column="room_state" type="java.lang.String" />
		<property name="keyState" column="key_state" type="java.lang.String" />
		<property name="unitName" column="unit_name" type="java.lang.String" />
		<property name="buildingName" column="building_name" type="java.lang.String" />
		<property name="areaName" column="area_name" type="java.lang.String" />
		<property name="commName" column="comm_name" type="java.lang.String" />
	</class> 
</hibernate-mapping>
