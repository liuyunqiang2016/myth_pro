<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.viatt.zhjtpro.bo">
	<class name="TblFingOpBo" table="tbl_fing_op">
		<composite-id>
			<key-property name="cardNo" column="card_no" type="java.lang.String" />
			<key-property name="opType" column="op_type" type="java.lang.String" />
			<key-property name="equCode" column="equ_code" type="java.lang.String" />
